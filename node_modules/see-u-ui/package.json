{
  "name": "see-u-ui",
  "version": "1.0.012",
  "description": "SeeYouUI，是全面兼容nvue的uni-app生态框架",
  "license": "MIT",
  "author": "GmhLovEDM",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.28",
    "@vitejs/plugin-vue": "^6.0.2",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vue": "3",
    "vue-tsc": "^3.1.5"
  }
}
